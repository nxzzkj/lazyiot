<?xml version="1.0" encoding="utf-8" ?>
<IOServer>
  <!--定义本地端口号-->
  <LocalPort>
    8888
  </LocalPort>
  <!--定义本地MDS服务端口号,默认是10905-->
  <MDSLocalPort>
    10905
  </MDSLocalPort>
  <WebUrl>
    http://192.168.43.121/
  </WebUrl>
  <!--定义管理员账户-->
  <User>
    admin
  </User>
  <!--管理员账户密码-->
  <Password>
    123456
  </Password>
  <HeartBeat>
    HH
  </HeartBeat>
  <HeadPack>
    ZZTEC
  </HeadPack>
  <TailPack>
    SCADA
  </TailPack>
  <ReceiveTimeout>
    20000
  </ReceiveTimeout>
  <ReceiveBufferSize>
    10240
  </ReceiveBufferSize>
  <SendTimeout>
    20000
  </SendTimeout>
  <SendBufferSize>
    10240
  </SendBufferSize>
  <DelayTime>
    200
  </DelayTime>
  <!--时序数据库配置文件-->
  <InfluxDBConfig> 
  </InfluxDBConfig>
  <!--时序数据库当前应用的数据库名称-->
  <InfluxDBGlobal>
    <DataBaseName>
      zzscadabase
    </DataBaseName>
    <User>root</User>
    <Password>root</Password>
      <InfluxDBVersion>v_1_3</InfluxDBVersion>
  </InfluxDBGlobal>

  <Backups>
    <Enable></Enable>
    <!--备份数据库的路径-->
    <BackupFullPath></BackupFullPath>
    <!--备份周期 每天,每周,每月-->
    <BackupCycle>每天</BackupCycle>
    <!--具体当天备份的时间点-->
    <BackupTime>23:59:59</BackupTime>
    <LastBackupDate></LastBackupDate>
  </Backups>
</IOServer>
